	@RequestMapping("/getUserInfo")
	public ResponseBase getUserInfo() {
		System.out.println(" 我是会员服务,会员服务调用订单服务开始啦！！");
		try {
			// 会员服务接口产生1.5秒的延迟
			Thread.sleep(1500);
		} catch (Exception e) {
			// TODO: handle exception
		}
		return setResultSuccess("订单服务接口调用会员服务接口成功....");
	}
}
